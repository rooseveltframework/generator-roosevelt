{
  "name": "sample-app",
  "description": "",
  "author": "",
  "version": "0.1.0",
  "homepage": "",
  "license": "",
  "main": "app.js",
  "readmeFilename": "",
  "engines": {
    "node": ">=0.10.0"
  },
  "engineStrict": true,
  "dependencies": {
    "roosevelt": "^0.6.0",
    "teddy": "^0.3.0",
    "roosevelt-less": "^0.2.0",
    "roosevelt-closure": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "*",
    "supervisor": "*"
  },
  "rooseveltConfig": {
    "port": 43711,
    "localhostOnly": true,
    "disableLogger": false,
    "noMinify": false,
    "multipart": {
      "multiples": true
    },
    "maxLagPerRequest": 2000,
    "shutdownTimeout": 30000,
    "modelsPath": "mvc/models",
    "modelsNodeModulesSymlink": "models",
    "viewsPath": "mvc/views",
    "viewEngine": "html: teddy",
    "controllersPath": "mvc/controllers",
    "error404": "404.js",
    "error5xx": "5xx.js",
    "error503": "503.js",
    "staticsRoot": "statics",
    "cssPath": "css",
    "cssCompiler": {
      "nodeModule": "roosevelt-less",
      "params": {
        "compress": true
      }
    },
    "cssCompilerWhitelist": null,
    "cssCompiledOutput": ".build/css",
    "jsPath": "js",
    "jsCompiler": {
      "nodeModule": "roosevelt-closure",
      "params": {
        "compilation_level": "ADVANCED_OPTIMIZATIONS"
      }
    },
    "jsCompilerWhitelist": null,
    "jsCompiledOutput": ".build/js",
    "publicFolder": "public",
    "favicon": "images/favicon.ico",
    "symlinksToStatics": [
      "css: .build/css",
      "images",
      "js: .build/js"
    ],
    "versionedStatics": false,
    "versionedCssFile": null,
    "alwaysHostPublic": false
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "eqeqeq": 2,
      "curly": 2,
      "semi": 2,
      "strict": [2, "never"],
      "quotes": [2, "single"],
      "no-trailing-spaces": [0, {"skipBlankLines": true}],
      "camelcase": [2, {"properties": "always"}],
      "comma-spacing": [2, {"before": false, "after": true}],
      "indent": [2, 2, {"VariableDeclarator": 2, "SwitchCase": 1}],
      "eol-last": 0,
      "handle-callback-err": 0,
      "no-use-before-define": [2, "nofunc"],
      "no-unused-vars": [2, {"vars": "all", "args": "none"}],
      "no-console": 0,
      "no-mixed-requires": 0,
      "one-var": [2, "always"],
      "dot-notation": 0,
      "no-path-concat": 0
    }
  },
  "private": true,
  "repository": {
    "private-repo": "git+ssh://somewhere:port/folder/on/server"
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "supervisor -k -e js,json,less,html -i statics/.build/,.git/,public/,node_modules/ -- app.js -prod",
    "dev": "supervisor -k -e js,json,less,html -i statics/.build/,.git/,public/,node_modules/ -- app.js -dev",
    "test": "eslint ."
  }
}
