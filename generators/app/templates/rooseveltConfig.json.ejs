{
  "makeBuildArtifacts": <%- makeBuildArtifacts %>,
  <% if (!staticSiteMode) { %>
    "http": <%- JSON.stringify(http) %>,
    "https": <%- JSON.stringify(https) %>,
    "secretsPath": "<%= secretsPath %>",
    "favicon": "images/favicon.ico",
    "modelsPath": <%- JSON.stringify(modelsPath) %>,
    "viewsPath": <%- JSON.stringify(viewsPath) %>,
    "controllersPath": <%- JSON.stringify(controllersPath) %>,
  <% } %>
  "viewEngine": <%- JSON.stringify(viewEngine) %>,
  "css": {
    "sourcePath": "css",
    "compiler": <%- JSON.stringify(cssCompilerOptions) %>,
    "output": "css",
    "versionFile": null
  },
  "js": {
    "sourcePath": "js",
    "webpack": {
      "enable": <%- webpackEnable %>,
      "bundles": <%- JSON.stringify(webpackBundle) %>
    }
  },
  <% if (spaMode) { %>
    "clientControllers": <%- JSON.stringify(clientControllers) %>,
    "clientViews": <%- JSON.stringify(clientViews) %>,
  <% } %>
  "symlinks": <%- JSON.stringify(symlinks) %>
}
