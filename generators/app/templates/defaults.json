{
  "dependencies": {
    "roosevelt": "~0.12.0"
  },
  "appName": "My Roosevelt Sample App",
  "createDir": true,
  "httpPort": 43711,
  "https": {
    "enable": false,
    "httpsOnly": false,
    "httpsPort": 43733,
    "pfx": false,
    "keyPath": "null",
    "passphrase": "null",
    "ca": "null",
    "requestCert": false,
    "rejectUnauthorized": false
  },
  "stateName": "n/a",
  "localityName": "n/a",
  "organizationName": "n/a",
  "organizationalUnit": "n/a",
  "altURI": "http://localhost/",
  "altIP": "127.0.0.1",
  "defaultCSSCompiler": "rooseveltLess",
  "rooseveltLess": {
    "dependencies": {
      "roosevelt-less": "~0.7.0"
    },
    "options": {
      "nodeModule": "roosevelt-less",
      "params": {
        "cleanCSS": {
          "advanced": true,
          "aggressiveMerging": true
        },
        "sourceMap": null
      }
    },
    "scripts": {
      "cssExt": "less",
      "cssSyntax": " --syntax less"
    }
  },
  "rooseveltSass": {
    "dependencies": {
      "roosevelt-sass": "~0.4.0"
    },
    "options": {
      "nodeModule": "roosevelt-sass",
      "params": {
        "outputStyle": "compressed"
      }
    },
    "scripts": {
      "cssExt": "scss",
      "cssSyntax": " --syntax scss"
    }
  },
  "defaultJSCompiler": "rooseveltUglify",
  "rooseveltUglify": {
    "dependencies": {
      "roosevelt-uglify": "~0.3.0"
    },
    "options": {
      "nodeModule": "roosevelt-uglify",
      "showWarnings": false,
      "params": {}
    }
  },
  "rooseveltClosure": {
    "dependencies": {
      "roosevelt-closure": "~0.5.1"
    },
    "options": {
      "nodeModule": "roosevelt-closure",
      "showWarnings": false,
      "params": {
        "compilationLevel": "ADVANCED"
      }
    }
  },
  "modelsPath": "mvc/models",
  "viewsPath": "mvc/views",
  "controllersPath": "mvc/controllers",
  "templatingEngine": true,
  "templatingEngineName": "teddy",
  "templatingExtension": "html",
  "additionalTemplatingEngines": false,
  "teddy": {
    "teddy": "~0.4.27"
  },
  "viewEngine": [
    "html:teddy"
  ]
}
