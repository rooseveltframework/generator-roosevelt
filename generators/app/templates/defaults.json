{
  "dependencies": {
    "roosevelt": "~0.19.0"
  },
  "appName": "My Roosevelt Sample App",
  "createDir": true,
  "httpsPort": "Random",
  "https": true,
  "altURI": "https://localhost/",
  "altIP": "127.0.0.1",
  "defaultCSSCompiler": "Less",
  "Less": {
    "dependencies": {
      "less": "~4.1.0"
    },
    "config": {
      "enable": true,
      "module": "less",
      "options": {}
    },
    "scripts": {
      "cssExt": "less",
      "stylelintConfigModule": "\"stylelint-config-standard\": \"~26.0.0\"",
      "stylelintConfigName": "stylelint-config-standard",
      "stylelintPostCssModule": "\"postcss-less\": \"~6.0.0\",",
      "stylelintSyntax": "\"customSyntax\": \"postcss-less\","
    }
  },
  "Sass": {
    "dependencies": {
      "sass": "~1.53.0"
    },
    "config": {
      "enable": true,
      "module": "sass",
      "options": {}
    },
    "scripts": {
      "cssExt": "scss",
      "stylelintPostCssModule": null,
      "stylelintConfigModule": "\"stylelint-config-standard-scss\": \"~4.0.0\"",
      "stylelintConfigName": "stylelint-config-standard-scss"
    }
  },
  "webpackBundle": [
    {
      "config": {
        "entry": "${js.sourcePath}/main.js",
        "output": {
          "path": "${publicFolder}/js"
        },
        "resolve": {
          "alias": {
            "fs": false,
            "path": false
          },
          "modules": [
            "${js.sourcePath}",
            "${publicFolder}/js",
            "${appDir}",
            "node_modules"
          ]
        }
      }
    }
  ],
  "modelsPath": "mvc/models",
  "viewsPath": "mvc/views",
  "controllersPath": "mvc/controllers",
  "templatingEngine": true,
  "templatingEngineName": "teddy",
  "templatingExtension": "html",
  "additionalTemplatingEngines": false,
  "teddy": {
    "teddy": "~0.5.0"
  },
  "viewEngine": [
    "html:teddy"
  ]
}
