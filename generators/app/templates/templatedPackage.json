{
  "name": "<%= appName %>",
  "description": "",
  "author": "",
  "version": "0.1.0",
  "homepage": "",
  "license": "",
  "main": "app.js",
  "readmeFilename": "",
  "engines": {
    "node": ">=4.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "roosevelt": "^0.8.0",
    "teddy": "^0.4.0",
    "roosevelt-less": "^0.2.0",
    "roosevelt-closure": "^0.3.0"
  },
  "devDependencies": {
    "standard": "^10.0.0",
    "lesshint": "^3.3.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "supervisor": "^0.12.0",
    "lint-staged": "^4.0.0",
    "husky": "^0.13.4"
  },
  "rooseveltConfig": {
    "port": <%= port %>,
    "localhostOnly": <%= localhostOnly %>,
    "suppressLogs": <%- suppressLogs %>,
    "noMinify": <%= noMinify %>,
    "enableValidator": <%= enableValidator %>,
    "htmlValidator": <%- htmlValidator %>,
    "validatorExceptions": <%- validatorExceptions %>,
    "multipart": <%- multipart %>,
    "maxLagPerRequest": 5000,
    "shutdownTimeout": <%= shutdownTimeout %>,
    "https": <%= https %>,
    "httpsOnly": <%= httpsOnly %>,
    "httpsPort": <%= httpsPort %>,
    "pfx": <%= pfx %>,
    "keyPath": <%- keyPath %>,
    "passphrase": <%- passphrase %>,
    "ca": <%- ca %>,
    "requestCert": <%- requestCert %>,
    "rejectUnauthorized": <%= rejectUnauthorized %>,
    "modelsPath": "<%= modelsPath %>",
    "viewsPath": "<%= viewsPath %>",
    "viewEngine": "<%= viewEngine %>",
    "controllersPath": "<%= controllersPath %>",
    "error404": "<%= error404 %>",
    "error5xx": "<%= error5xx %>",
    "error503": "<%= error503 %>",
    "staticsRoot": "<%= staticsRoot %>",
    "htmlMinify": <%- htmlMinify %>,
    "cssPath": "<%= cssPath %>",
    "cssCompiler": <%- cssCompiler %>,
    "cssCompiledOutput": "<%= cssCompiledOutput %>",
    "cssCompilerWhitelist": <%- cssCompilerWhitelist %>,
    "jsPath": "<%= jsPath %>",
    "bundledJsPath": "<%= bundledJsPath %>",
    "exposeBundles": <%= exposeBundles %>,
    "browserifyBundles": [],
    "jsCompiler": <%- jsCompiler %>,
    "jsCompiledOutput": "<%= jsCompiledOutput %>",
    "jsCompilerWhitelist": <%- jsCompilerWhitelist %>,
    "publicFolder": "<%= publicFolder %>",
    "favicon": "<%= favicon %>",
    "symlinksToStatics": <%- symlinksToStatics %>,
    "versionedStatics": <%= versionedStatics %>,
    "versionedCssFile": <%- versionedCssFile %>,
    "alwaysHostPublic": <%= alwaysHostPublic %>,
    "bodyParserUrlencodedParams": <%- bodyParserUrlencodedParams %>,
    "bodyParserJsonParams": <%- bodyParserJsonParams %>
  },
  "private": true,
  "repository": {
    "private-repo": "git+ssh://somewhere:port/folder/on/server"
  },
  "scripts": {
    "start": "npm run prod",
    "kill-validator": "node ./node_modules/roosevelt/lib/killValidator.js",
    "prod": "./node_modules/.bin/supervisor -k -e js,json,less,html -i <%= buildPath %>/,.git/,<%= publicFolder %>/,node_modules/,jsdoc/ -- app.js -prod",
    "dev": "./node_modules/.bin/supervisor -k -e js,json,less,html -i <%= buildPath %>/,.git/,<%= publicFolder %>/,node_modules/,jsdoc/ -- app.js -dev",
    "cleanup": "node ./node_modules/roosevelt/lib/appCleanup.js",
    "audit": "node ./node_modules/roosevelt/lib/configAuditor.js",
    "test": "npm run lint",
    "eslint": "standard",
    "stylelint": "./node_modules/.bin/stylelint \"statics/css/**/*.less\" --syntax less",
    "lint": "npm run stylelint && npm run eslint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.less": "./node_modules/.bin/stylelint --syntax less",
    "*.js": "standard"
  }
}
