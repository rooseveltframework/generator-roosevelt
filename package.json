{
  "name": "generator-roosevelt",
  "description": "Yeoman generator for Roosevelt apps",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/generator-roosevelt/graphs/contributors"
    }
  ],
  "version": "0.20.0",
  "files": [
    "generators"
  ],
  "homepage": "https://github.com/rooseveltframework/generator-roosevelt",
  "license": "CC-BY-4.0",
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "gulp-beautify": "~3.0.0",
    "gulp-filter": "~7.0.0",
    "selfsigned": "~2.0.1",
    "terminal-link": "~2.1.1",
    "yeoman-generator": "~5.6.1"
  },
  "devDependencies": {
    "c8": "~7.12.0",
    "codecov": "~3.8.2",
    "eslint": "~8.23.0",
    "eslint-plugin-mocha": "~10.1.0",
    "mocha": "~10.0.0",
    "roosevelt": "~0.20.0",
    "standard": "~17.0.0",
    "yeoman-environment": "~3.9.1",
    "yeoman-test": "~6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/generator-roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "preferGlobal": true,
  "_from": "generator-roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "mocha": {
    "timeout": "10s"
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha test/unit",
    "lint": "eslint ./test && standard",
    "test": "mocha test/unit",
    "test-debug": "mocha --inspect-brk test/unit"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/kethinov"
  }
}
