{
  "name": "generator-mkroosevelt",
  "description": "Generator for Roosevelt apps.",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.1.49",
  "homepage": "https://github.com/kethinov/mkroosevelt",
  "license": "CC-BY-4.0",
  "main": "mkroosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=4.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "yeoman-generator": "^0.24.1",
    "yeoman-assert": "^2.2.2",
    "yeoman-test": "^1.6.0",
    "fs-extra": "^1.0.0",
    "update-notifier": "^1.0.2",
    "node-forge": "^0.6.42",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "mocha": "^3.2.0",
    "is-my-json-valid": "^2.15.0",
    "chai": "^3.5.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "eqeqeq": 2,
      "curly": 2,
      "semi": 2,
      "strict": [2, "never"],
      "quotes": [2, "single"],
      "no-trailing-spaces": ["error", {"skipBlankLines": false}],
      "camelcase": [2, {"properties": "always"}],
      "comma-spacing": [2, {"before": false, "after": true}],
      "indent": [2, 2, {"VariableDeclarator": 2}],
      "eol-last": 0,
      "handle-callback-err": 0,
      "no-use-before-define": [2, "nofunc"],
      "no-unused-vars": [2, {"vars": "all", "args": "none"}],
      "no-console": 0,
      "no-mixed-requires": 0,
      "one-var": [2, "always"],
      "dot-notation": 0,
      "no-path-concat": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kethinov/mkroosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "io.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "bin": {
    "mkroosevelt": "mkroosevelt.js"
  },
  "preferGlobal": true,
  "_from": "mkroosevelt@*",
  "gitHead": "",
  "scripts": {
    "lint": "eslint *",
    "test": "./node_modules/.bin/mocha unitTest.js"
  },
  "files": [
    "generators/app"
  ]
}
