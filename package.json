{
  "name": "generator-roosevelt",
  "description": "Yeoman generator for Roosevelt apps",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/generator-roosevelt/graphs/contributors"
    }
  ],
  "version": "0.22.3",
  "files": [
    "generators"
  ],
  "homepage": "https://github.com/rooseveltframework/generator-roosevelt",
  "license": "CC-BY-4.0",
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "gulp-beautify": "3.0.0",
    "gulp-filter": "9.0.1",
    "selfsigned": "2.4.1",
    "terminal-link": "3.0.0",
    "yeoman-generator": "5.10.0"
  },
  "devDependencies": {
    "c8": "10.1.2",
    "codecov": "3.8.3",
    "eslint": "9.8.0",
    "eslint-plugin-mocha": "10.4.3",
    "mocha": "10.4.0",
    "roosevelt": "0.22.14",
    "standard": "17.1.0",
    "yeoman-environment": "3.19.3",
    "yeoman-test": "8.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/generator-roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "preferGlobal": true,
  "_from": "generator-roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "mocha": {
    "timeout": "10s"
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov mocha test/unit",
    "lint": "eslint ./test && standard",
    "test": "mocha test/unit",
    "test-debug": "mocha --inspect-brk test/unit"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/kethinov"
  }
}
