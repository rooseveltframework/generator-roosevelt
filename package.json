{
  "name": "generator-roosevelt",
  "description": "Yeoman generator for Roosevelt apps",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.2.5",
  "homepage": "https://github.com/rooseveltframework/generator-roosevelt",
  "license": "CC-BY-4.0",
  "main": "generators/app/index.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "fs-extra": "^1.0.0",
    "node-forge": "^0.6.49",
    "prompt": "^1.0.0",
    "update-notifier": "^1.0.3",
    "yeoman-assert": "^2.2.3",
    "yeoman-generator": "^0.24.1",
    "yeoman-test": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "mocha": "^3.2.0",
    "is-my-json-valid": "^2.15.0",
    "chai": "^3.5.0",
    "lint-staged": "^4.0.0",
    "husky": "^0.13.4"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "eqeqeq": 2,
      "curly": 2,
      "semi": 2,
      "strict": [
        2,
        "never"
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": false
        }
      ],
      "camelcase": [
        2,
        {
          "properties": "always"
        }
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "indent": [
        2,
        2,
        {
          "VariableDeclarator": 2
        }
      ],
      "eol-last": 0,
      "handle-callback-err": 0,
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-console": 0,
      "no-mixed-requires": 0,
      "one-var": [
        2,
        "always"
      ],
      "dot-notation": 0,
      "no-path-concat": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/generator-roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "io.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "preferGlobal": true,
  "_from": "generator-roosevelt@*",
  "gitHead": "",
  "scripts": {
    "lint": "eslint .",
    "test": "set NODE_ENV=test && \"./node_modules/.bin/mocha\" unitTest.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "./node_modules/.bin/eslint"
  },
  "files": [
    "generators/app",
    "sampleApp",
    "rooseveltDefaults.json"
  ]
}
