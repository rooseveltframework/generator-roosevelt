{
  "name": "generator-roosevelt",
  "description": "Yeoman generator for Roosevelt apps",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    "Eric Newport <kethinov@gmail.com>",
    "Troy Coutu <autre31415@gmail.com>",
    "Alexander J. Lallier <alexanderlallier@aol.com>"
  ],
  "version": "0.4.1",
  "homepage": "https://github.com/rooseveltframework/generator-roosevelt",
  "license": "CC-BY-4.0",
  "main": "generators/app/index.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "node-forge": "0.7.6",
    "yeoman-generator": "3.0.0"
  },
  "devDependencies": {
    "codecov": "3.0.4",
    "eslint": "5.0.1",
    "eslint-plugin-mocha": "5.0.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "standard": "11.0.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/generator-roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "io.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "preferGlobal": true,
  "_from": "generator-roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "coverage": "nyc mocha test/unit",
    "lint": "standard && eslint ./test",
    "precommit": "lint-staged",
    "test": "npm run lint && mocha test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  }
}
