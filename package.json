{
  "name": "generator-roosevelt",
  "description": "Yeoman generator for Roosevelt apps",
  "author": "Roosevelt Framework Team <rooseveltframework@gmail.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/rooseveltframework/generator-roosevelt/graphs/contributors"
    }
  ],
  "version": "0.7.0",
  "homepage": "https://github.com/rooseveltframework/generator-roosevelt",
  "license": "CC-BY-4.0",
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "dependencies": {
    "gulp-beautify": "3.0.0",
    "gulp-filter": "6.0.0",
    "node-forge": "0.8.5",
    "yeoman-generator": "4.0.1"
  },
  "devDependencies": {
    "codecov": "~3.5.0",
    "eslint": "~6.0.1",
    "eslint-plugin-mocha": "~5.3.0",
    "husky": "~3.0.0",
    "lint-staged": "~9.2.0",
    "mocha": "~6.1.4",
    "nyc": "~14.1.1",
    "standard": "~14.0.0",
    "yeoman-assert": "~3.1.1",
    "yeoman-test": "~2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/generator-roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "io.js",
    "roosevelt",
    "teddy",
    "mvc",
    "yeoman-generator"
  ],
  "preferGlobal": true,
  "_from": "generator-roosevelt@*",
  "gitHead": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "plugins": [
      "mocha"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error"
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "lcov"
    ]
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "coverage": "nyc mocha test/unit",
    "lint": "standard && eslint ./test",
    "test": "mocha test/unit"
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint ./test"
    }
  }
}
